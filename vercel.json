{
  "version": 2,
  "builds": [
    {
      "src": "install-chrome-deps.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
